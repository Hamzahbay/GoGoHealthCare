    <% if( pasien.no != '' ) { %>
            <!-- Navigation Bar -->
            <div class="navbar white">
                <span class="nav-title"><strong>GoGo Health<em>care</em></strong></span>
                <i class="material-icons">view_headline</i>
                <div class="menu">
                    <a class="deco-none white" href="/"><span>Beranda</span></a>
                    <a class="deco-none white" href="/pasien"><span>Tanya Dokter</span></a>
                    <a class="deco-none white" href="/info_kesehatan"><span>Info Kesehatan</span></a>
