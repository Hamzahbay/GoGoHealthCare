                    <a class="deco-none white" href="/logout"><div>Keluar</div></a>
                </div>
            </div>

            <!-- Content -->
            <div class="content">
                <div class="title text-center green size-xxlarge"><p>Buat Janji di Bulan <span></span></p></div>
                <div class="box text-center">
                    <div class="alert-box text-center">
                        <%- include('./../../partials/messages') %>
                    </div>
                    <form action="/buat_janji/form_janji" method="POST">
                        <p class="input">
                            Nama:
                            <input type="text" name="nama" id="name" placeholder="Nama" value="<%= pasien.nama %>" required autocomplete="off">
                            No Telfon:
                            <input type="text" name="telfon" id="" placeholder="No Telfon/Hp/WA" value="<%= pasien.telfon %>" required autocomplete="off">
                        </p>
                        <p class="input">
                            Alamat:
                            <input type="text" name="alamat" id="address" placeholder="Alamat" value="<%= pasien.alamat %>" required autocomplete="off">
                            Tanggal Janji:
                            <input type="date" name="tanggal" id="dateInput" required autocomplete="off" placeholder="Pilih Tanggal">
                        </p>
                        <p>
                            <p>Riwayat Penyakit:</p>
                            <textarea name="riwayat" id="message1" placeholder=" Ceritakan Riwayat Penyakit Anda..." rows="5"></textarea>
                            <p>Keluhan:</p>
                            <textarea name="keluhan" id="message2" placeholder=" Ceritakan Keluhan Anda..." rows="5"></textarea>
                        </p>
                        <p class="submit"><input type="reset" value="Reset"><input type="submit" value="Cetak"></p>
                    </form>
                </div>
                <div class="alert text-center green">
                    <p class="size-xxlarge">Perhatian !</p>
                    <div class="size-large">
                        Mohon isi Form Janji dengan Sebenar-benarnya.
                    </div>
                </div>
            </div>
